{
    "manifest_version": 2,
    "name": "Youtube Music Video Disabler",
    "version": "1.0",
    "description": "Removes music videos from youtube music.",
    "icons": {
        "48": "icons/icon-48.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://music.youtube.com/*"
            ],
            "js": [
                "disabler.js"
            ]
        }
    ]
}